# Dev convenience: prefill login form credentials
# Copy this file to .env.local and fill in your values
# .env.local is gitignored and won't be committed

# Prefill email/password fields on login form
VITE_DEV_EMAIL=travis@example.com
VITE_DEV_PASSWORD=travis

# Set to "true" to auto-submit login form (zero clicks)
VITE_DEV_AUTOLOGIN=true

# Optional: Override API URL (defaults to http://localhost:4000/graphql)
# VITE_API_URL=http://localhost:4000/graphql
